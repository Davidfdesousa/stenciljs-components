{
  "name": "web-components-lib",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/web-components-lib/web-components-lib.esm.js",
  "exports": {
    ".": {
      "import": "./dist/web-components-lib/web-components-lib.esm.js",
      "require": "./dist/web-components-lib/web-components-lib.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build:css": "sass src/globals/app.scss www/globals/app.css --no-source-map",
    "build": "yarn build:css && stencil build",
    "start": "yarn build:css && stencil build --dev --watch --serve"
  },
  "devDependencies": {
    "@stencil/core": "^4.25.1",
    "@stencil/sass": "^3.0.12",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0",
    "sass": "^1.83.4"
  },
  "license": "MIT"
}
